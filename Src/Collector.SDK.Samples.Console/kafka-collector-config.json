{
  "Version": "2.0",
  "Id": "1",
  "Type": "Collector.SDK.ActiveDirectoryCollector,Collector.SDK.ActiveDirectory",
  "StackType": "Collector.SDK.Collectors.Stack,Collector.SDK",
  "ThirdPartyTypes": [
    {
      "AssemblyName": "Collector.SDK",
      "Type": "LoggerFacade",
      "RegisterAll": false,
      "Owned": false,
      "Singleton": false
    }
  ],
  "ThirdPartyModules": [
    {
      "AssemblyName": "Autofac.Extras.NLog",
      "RegisterAll": true
    }
  ],
  "Properties": {
    "ReaderId": "Kafka-Reader"
  },
  "EndPoints": [
    {
      "Id": "Kafka-Endpoint",
      "Name": "Kafka Endpoint",
      "Properties": {
        "GroupId": "test",
        "Topic": "service_now_in",
        "BootstrapServers":  "10.0.0.223:9092"
      }
    },
    {
      "Id": "Log-Endpoint",
      "Name": "Published Log Files",
      "Properties": {
        "Path": "c:/programdata/ivanti/kafka"
      }
    }
  ],
  "Readers": [
    {
      "Id": "Kafka-Reader",
      "Type": "Collector.SDK.Kafka.Readers.KafkaReader,Collector.SDK.Kafka",
      "EndPointId": "Kafka-Endpoint",
      "DataType": "Device"
    }
  ],
  "Transformers": [
    {
      "Id": "Device-Transform",
      "Type": "Collector.SDK.Transformers.DefaultTransformer,Collector.SDK",
      "ReaderId": "Kafka-Reader",
      "Mappers": [
        {
          "Id": "Device-Mapper",
          "TransformerId": "Device-Transform",
          "Type": "Collector.SDK.ActiveDirectory.Mappers.PipedMapper,Collector.SDK.ActiveDirectory",
          "DataType": "Collector.SDK.Samples.DataModels.Device,Collector.SDK.Samples.DataModel",
          "PipedConverters": [
            {
              "Id": "NameOnlyConverter",
              "Type": "Collector.SDK.Converters.NameOnlyConverter,Collector.SDK",
              "Properties": {
              }
            },
            {
              "Id": "DateConverter",
              "Type": "Collector.SDK.Converters.DateTimeUtcConverter,Collector.SDK",
              "Properties": {
              }
            }
          ],
          "SourceTargetMappings": [
            {
              "PrimaryKey": "id",
              "TargetConverters": [
                {
                  "Id": "NameOnlyConverter",
                  "NestOutput": "false",
                  "Pipe": "true",
                  "CombineInputOutput": "false",
                  "LeftSideMap": {
                    "id": [ "Id" ],
                    "name": [ "Name" ],
                    "created": [ "Created" ],
                    "changed": [ "Changed" ]
                  }
                },
                {
                  "Id": "DateConverter",
                  "NestOutput": "false",
                  "Pipe": "true",
                  "CombineInputOutput": "true",
                  "LeftSideMap": {
                    "Created": [ "CreatedUtc" ],
                    "Changed": [ "ChangedUtc" ]
                  }
                }
              ],
              "Properties": {}
            }
          ],
          "Properties": {}
        }
      ]
    }
  ],
  "Publishers": [
    {
      "Id": "Log-Publisher",
      "Type": "Collector.SDK.ActiveDirectory.Publishers.LogPublisher,Collector.SDK.ActiveDirectory",
      "TransformerId": "Device-Transform",
      "EndPointId": "Log-Endpoint"
    }
  ]
}
