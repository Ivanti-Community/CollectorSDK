<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <nlog>
    <targets>
      <target name="console" type="Console" layout="${message}"/>
      <target name="debugger" type="Debugger" layout="${message}"/>
      <target name="logfile" type="File" fileName="C:/ProgramData/Ivanti/ADConnector/Logs/AD-Connector.log"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="console,debugger,logfile"/>
    </rules>
  </nlog>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
    </startup>
</configuration>